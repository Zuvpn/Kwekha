[Unit]
Description=Kwekha Telegram Bot
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
ExecStart=/usr/local/bin/kwekha-telegram
Restart=always
RestartSec=2
StandardOutput=append:/var/log/kwekha/telegram.log
StandardError=append:/var/log/kwekha/telegram.log

[Install]
WantedBy=multi-user.target
